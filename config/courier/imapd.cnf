
RANDFILE = /usr/lib/courier/imapd.rand

[ req ]
default_bits = 1024
encrypt_key = yes
distinguished_name = req_dn
x509_extensions = cert_type
prompt = no

[ req_dn ]
C=US
ST=Oregon
L=Portland
O=Kineticode, Inc.
OU=Kineticode, Inc.
CN=mail.kineticode.com
emailAddress=postmaster@kineticode.com


[ cert_type ]
nsCertType = server
